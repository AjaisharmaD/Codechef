import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		
		Scanner sc= new Scanner(System.in);
		int n=sc.nextInt();
		int a,b=5,c;
		for(int i=0;i<n;i++)
		{
		    a=sc.nextInt();
		    a=a+a;
		    c=a*b;
		    System.out.println(c);
		}
	}
}
