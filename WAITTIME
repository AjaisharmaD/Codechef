import java.util.*;
import java.lang.*;
import java.io.*;


class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner sc=new Scanner(System.in);

		int a,b;
		n=sc.nextInt();
		for(int i=0;i<n;i++)
		{
		    a=sc.nextInt();
		    b=sc.nextInt();
		    a*=7;
		    int c=a-b;
		    System.out.println(c);
		}
	}
}
