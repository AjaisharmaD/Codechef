
import java.util.*;
import java.lang.*;
import java.io.*;

class Burger
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner sc= new Scanner(System.in);
	    int n,a,b;
	    n=sc.nextInt();
	    for(int i=0;i<n;i++)
	    {
	        a=sc.nextInt();
	        b=sc.nextInt();
	        if(a>b)
	        System.out.println(b);
	        else if(a<b)
	        System.out.println(a);
	        else if(a==b)
	        System.out.println(a);
	        
	    }
	  }
}
