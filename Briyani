/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

class Briyani
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int n;
		n=sc.nextInt();
		int a,b,c;
		for(int i=0;i<n;i++)
		{
		    a=sc.nextInt();
		    b=sc.nextInt();
		    c=a*b;
		    System.out.println(c);
		}
		
	}
}
